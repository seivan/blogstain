- form_for @user_session, :url => { :action => "create", :controller=> "user_sessions" } do |f|
  = f.error_messages
  %p
    = f.label :username, "UserName"
    = f.text_field :username
  %p 
    = f.label :password, "Password"
    = f.password_field :password
    
  %p= f.submit "Submit"#, :disable_with => 'Submiting...'
